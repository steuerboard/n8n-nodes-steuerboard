{
	"name": "@steuerboard/n8n-nodes-steuerboard",
	"version": "1.4.0",
	"description": "n8n community node to work with the Steuerboard API",
	"license": "MIT",
	"homepage": "https://steuerboard.net",
	"keywords": [
		"n8n-community-node-package",
		"steuerboard",
		"accounting",
		"steuerberater"
	],
	"author": {
		"name": "steuerboard",
		"email": "support@steuerboard.net"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/steuerboard/n8n-nodes-steuerboard"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "dotenv -e .env n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"release": "release-it",
		"release:dry": "release-it --dry-run",
		"pack:check": "npm pack --dry-run",
		"pack:preview": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/SteuerboardApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/Steuerboard/Steuerboard.node.js",
			"dist/nodes/Steuerboard/SteuerboardTrigger.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"@release-it/conventional-changelog": "^10.0.1",
		"@types/node": "^24.5.0",
		"dotenv-cli": "^10.0.0",
		"eslint": "9.35.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
